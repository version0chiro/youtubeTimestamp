{
  "name": "Youtube Timestamps",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Adds timestamps to Youtube videos",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["js/youtube.js"]
    }
  ],
  "browser_action": {
    "default_title": "Youtube Timestamps",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": ["tabs"]
}
